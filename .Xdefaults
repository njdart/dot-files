*background: #101010
*foreground: #b5b5b5
*color0:  #1c1c1c
*color1:  #d74b73
*color2 : #9acc79
*color3:  #d0d26b
*color4: #77b6c5
!*color4 : #323742
*color5:  #a488d9
*color6:  #7fcab3
!*color7:  #658289
*color7: #8d8d8d
*color8:  #3d3a3a
*color9:  #b94062
*color10: #8fb676
*color11: #c8bc45
*color12: #8fa7b9
*color13: #8888a1
*color14: #6ec2a8
*color15: #dad3d3

! fonts
! run "fc-list" for a list of available fonts

!URxvt*font: xft:Terminus:pixelsize=12
URxvt*font: xft:Source Code Pro:pixelsize=10
!URxvt*font: xft:Oxygen Mono:pixelsize=12
!URxvt*font: xft:Anonymous Pro:size=12

urxvt*transparent: true
urxvt*shading: 15

!needed for backspace?!?
URxvt*termName: xterm

!URxvt*scrollTtyOutput: False
URxvt*scrollTtyOutput: True
URxvt*scrollTtyKeypress: True
URxvt*secondaryScroll: True
URxvt*saveLines: 8000
URxvt*scrollBar: False

! Extensions
urxvt*perl-lib:        /usr/lib/urxvt/perl/
urxvt*perl-ext-common: default,matcher,searchable-scrollback
#urxvt*perl-ext-common: kuake
#URxvt.kuake.hotkey: F10
urxvt*urlLauncher:     /usr/bin/chrome
urxvt*matcher.button:  1
!  - catch ugly URLs
urxvt.cutchars:        `()'*<>[]{|}"
URxvt.url-launcher:      /usr/bin/google-chrome-stable
!URxvt.perl-ext-common : tabbed
!URxvt.tabbed.tabbar-fg: 5
!URxvt.tabbed.tabbar-bg: 0
!URxvt.tabbed.tab-fg:    14
!URxvt.tabbed.tab-bg:    0
